                    function all(pvalidationGroup) {
                        if (typeof (Page_Validators) != "undefined") {
                            for (var i = 0; i < Page_Validators.length; i++) {
                                if (Page_Validators[i].validationGroup == pvalidationGroup) {
                                    if (!Page_Validators[i].isvalid) {
